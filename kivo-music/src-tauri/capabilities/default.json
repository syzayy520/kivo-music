{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for main window",
  "windows": [
    "main"
  ],
  "remote": {
    "urls": [
      "http://localhost:*/*",
      "http://127.0.0.1:*/*"
    ]
  },
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "fs:default"
  ]
}